{
    "name": "m6web/fos-rest-extra-bundle",
    "type": "symfony-bundle",
    "description" : "Extra feature for the FOSRestBundle",
    "authors": [
        {
            "name": "Fabien de Saint pern"
        }
    ],
    "autoload": {
        "psr-4": { "M6Web\\Bundle\\": "src/" }
    },
    "config": {
      "bin-dir": "bin"
    },
    "minimum-stability" : "dev",
    "prefer-stable": true,
    "require": {
        "friendsofsymfony/rest-bundle"   : "~1.1||^2.0",
        "doctrine/common": "@stable"
    },
    "require-dev": {
        "atoum/atoum"       : "^2.8||^3.0",
        "atoum/atoum-bundle": "@stable",
        "m6web/coke"        : "~1.2||^2.2",
        "m6web/symfony2-coding-standard" : "~1.1||^2.0||^3.3"
    }
}
