{
    "name": "m6web/fos-rest-extra-bundle",
    "type": "symfony-bundle",
    "description" : "Extra feature for the FOSRestBundle",
    "authors": [
        {
            "name": "Fabien de Saint pern"
        }
    ],
    "autoload": {
        "psr-4": { "M6Web\\Bundle\\": "src/" }
    },
    "config": {
      "bin-dir": "bin"
    },
    "minimum-stability" : "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "friendsofsymfony/rest-bundle" : "~1.1||^2.0||^3.3",
        "doctrine/common": "@stable"
    },
    "require-dev": {
        "atoum/atoum"              : "^4.0",
        "m6web/php-cs-fixer-config": "2.0.*",
        "phpstan/phpstan"          : "0.12.*"
    }
}
